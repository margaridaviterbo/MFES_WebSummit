class Talk
types
	public TalkTitle = seq1 of char;
instance variables
	public Title: TalkTitle;
	public Speaker: User`UserName;
	public Date: Date;
	public Start: Time;
	public End: Time; 
operations
	public Talk: TalkTitle * User`UserName * Date * Time * Time ==> Talk
	Talk(title, speaker, date, startTime, endTime) == (
		Title := title;
		Speaker := speaker;
		Date := date;
		Start := startTime;
		End := endTime;
		return self;
	);
end Talk